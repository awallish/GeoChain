
def import_pods
  pod 'Alamofire', '~> 4.7'
  pod 'Alamofire-Synchronous', '~> 4.0'
  pod 'BigInt', '~> 3.0.1'
  pod 'Result', '~> 3.0.0'
  pod 'libsodium'
  pod 'CryptoSwift', '0.8.3'
  pod 'secp256k1_ios', :git => 'https://github.com/shamatar/secp256k1_ios.git'
  pod 'PromiseKit', '~> 4.4'
  pod 'AwaitKit', '~> 3.0.1'
  pod 'web3swift', '0.3.5'
end



target 'crypto' do
  platform :ios, '9.0'
  use_frameworks!
  
  import_pods
  

end